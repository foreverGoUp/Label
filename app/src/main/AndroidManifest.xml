<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kc.label">

    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- 危险权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 用于读取手机当前的状态 -->
    <uses-permission android:name="android.permission.CAMERA" />

    <!-- 危险权限 -->
    <application
        android:name="com.kc.util.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.kc.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kc.activity.CubicleListActivity" />
        <activity android:name="com.kc.activity.DeviceListActivity" />
        <activity android:name="com.kc.activity.CustomScanActivity" />
        <activity android:name="com.kc.activity.WelcomeActivity" />
        <activity android:name="com.kc.activity.openfile.PdfActivity" />
        <activity android:name="com.kc.activity.openfile.PPTActivity" />
        <activity android:name="com.kc.activity.ViewSvgActivity" />
        <activity android:name="com.kc.activity.ClearCacheActivity"></activity>
    </application>

</manifest>